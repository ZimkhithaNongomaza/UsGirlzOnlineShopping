                <img src="~/Content/Images/bottoms.png" />
            </div>
        </div>
    </a>

    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">
        <div class="card text-white @*bg-info*@ mb-3" style="max-width: 18rem; background-color: rebeccapurple ">

            <div class="card-body">
                <h5 class="card-title" style=" font-family: Georgia, serif;">DRESSES</h5>
                <img src="~/Content/Images/dress.png" />
            </div>
        </div>
    </a>

    
</div>

